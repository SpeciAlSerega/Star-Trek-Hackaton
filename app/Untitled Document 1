from openpyxl import load_workbook
wb = load_workbook('./Data set.xlsx')
sheet = wb.get_sheet_by_name('Лист')
from app_pet.models import *
def aaa(az):
	if (az!=None and az!="Null"):
		return az
	else: 
		return ""
z=0
for row in range(3,244):
	a  = PetModel(card_pet=sheet.cell(row=row, column=2).value, type_pet=sheet.cell(row=row, column=3).value, age=int(sheet.cell(row=row, column=4).value), weight = int(sheet.cell(row=row, column=5).value), nickname=sheet.cell(row=row, column=6).value, sex= sheet.cell(row=row, column=7).value, breed_of_dog=sheet.cell(row=row, column=8).value, color=sheet.cell(row=row, column=9).value, fur=sheet.cell(row=row, column=10).value, ears=sheet.cell(row=row, column=11).value, tail = sheet.cell(row=row, column=12).value, size=sheet.cell(row=row, column=13).value, special_signs = sheet.cell(row=row, column=14).value, aviary_number=int(sheet.cell(row=row, column=15).value), identification_mark=int(sheet.cell(row=row, column=16).value), sterilization_date=sheet.cell(row=row, column=17).value, veterinarian=sheet.cell(row=row, column=18).value, socialized=sheet.cell(row=row, column=19).value, act_work_order=sheet.cell(row=row, column=20).value, data_work_order=sheet.cell(row=row, column=21).value, capture_act=sheet.cell(row=row, column=23).value, catching_address=sheet.cell(row=row, column=24).value, date_admission = sheet.cell(row=row, column=28).value, act_admission=sheet.cell(row=row, column=29).value, date_leaving= aaa(sheet.cell(row=row, column=30).value), reason_leaving = aaa(sheet.cell(row=row, column=31).value), act_leaving= aaa(sheet.cell(row=row, column=32).value), date_inspection = aaa(sheet.cell(row=row, column=45).value), anamnesis = aaa(sheet.cell(row=row, column=46).value))
	a.save()
	

